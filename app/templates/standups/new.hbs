<form class="container" onSubmit= {{route-action "submit"}}>
  <h2>Standups</h2>

  <table>
    <thead>
      <tr className="flexi-row">
        <th></th>
        <th>Done</th>
        <th>Next</th>
        <th>Help</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.state.currentStandup.teamMembers as |teamMember|}}
        {{team-member-input teamMember=teamMember registerChild=(route-action 'registerInputGrabber')}}
      {{/each}}
    </tbody>
  </table>

  <button>Submit</button>
</form>
